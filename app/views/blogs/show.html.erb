<% content_for :title do %>
  Quill: <%= @user.username %>
<% end %>

<%= react_component('BlogHeading',
  user: @user,
  profile_pic_url: @user.guaranteed_profile_pic_url
) %>

<%= render partial: "relationships/follow_unfollow_buttons", locals: { user: @user } %>

<%= react_component('PostList', 
  initial_state: JSON.parse(render partial: "api/dashboard/dashboard.json", locals: { posts: @posts }),
  big_title: false,
  collapsible_reblogs: true,
  display_author: false) %>
