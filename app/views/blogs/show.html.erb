<h1><%= link_to @user.username, blog_url(@user) %>'s blog!!!</h1>

<% @posts.each do |post| %>
  <div class="panel panel-default">
    <div class="panel-body">
      <h2><%= link_to post.title, blog_post_url(post.user, post) %></h2>
      <hr>
      <%= markdown.render(post.body).html_safe %>
    </div>
  </div>
<% end %>