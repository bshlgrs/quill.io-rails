<% if current_user %>
  <%= render partial: "posts/new_text_post", locals: { collapsible: true } %>
<% end %>

<%= react_component('PostList', 
  initial_state: JSON.parse(render partial: "api/dashboard/dashboard.json", locals: { posts: @posts }),
  current_user: current_user,
  big_title: false,
  collapsible_reblogs: true,
  display_author: true,
  list_name: "DashboardPostList") %>
